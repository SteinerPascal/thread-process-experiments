# command to compile ThreadSpawn.c
gcc -pthread -o threadspawn.out ThreadSpawn.c

# command to compile ProcessSpawn.c
gcc -o processspawn.out ProcessSpawn.c

#compile ProcessSharing.c
gcc -o processsharing.out ProcessSharing.c

#compile ThreadSharing.c
gcc -pthread -o threadsharing.out ThreadSharing.c
#compile test
gcc -o test.out Test.c
